{
	"name":"re:tab",
	"short_name":"Wheee",
	"description":"Save a tab, and we will reopen it for you!",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Save for Later!"
	},

	"permissions": [
		"activeTab",
		"windows",
		"tabs",
		"storage",
		"alarms",
		"notifications"
	],

	"background": {
		"page":"background.html"
	}
	
}